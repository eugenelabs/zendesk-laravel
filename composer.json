{
    "name": "eugenelabs/zendesk-laravel",
    "type": "library",
    "description": "Laravel wrapper for zendesk/zendesk_api_client_php package",
    "keywords": [
        "zendesk"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Cristhian Hernandez",
            "email": "cristhian@eugenelabs.com"
        }
    ],
    "require": {
        "php": ">=8.3",
        "illuminate/support": "^11.0|^12.0",
        "zendesk/zendesk_api_client_php": "2.*|^3.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Eugenelabs\\Zendesk\\Providers\\ZendeskServiceProvider"
            ],
            "aliases": {
                "Zendesk": "Eugenelabs\\Zendesk\\Facades\\Zendesk"
            }
        }
    },
    "autoload": {
        "psr-4": {
            "Eugenelabs\\Zendesk\\": "src"
        }
    },
    "minimum-stability": "stable"
}
